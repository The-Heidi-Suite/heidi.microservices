{
  "id": null,
  "uid": "heidi-microservices",
  "title": "HEIDI Microservices",
  "tags": ["microservices", "heidi"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "type": "graph",
      "title": "HTTP Request Rate",
      "targets": [
        {
          "expr": "sum(rate(heidi_http_requests_total[5m])) by (service)",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "yaxes": [{ "format": "reqps" }, { "format": "short" }],
      "xaxis": { "mode": "time", "show": true }
    },
    {
      "id": 2,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "type": "graph",
      "title": "HTTP Request Duration (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(heidi_http_request_duration_seconds_bucket[5m])) by (service, le))",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "yaxes": [{ "format": "s" }, { "format": "short" }],
      "xaxis": { "mode": "time", "show": true }
    },
    {
      "id": 3,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
