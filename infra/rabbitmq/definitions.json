{
  "rabbit_version": "3.13.0",
  "rabbitmq_version": "3.13.0",
  "product_name": "RabbitMQ",
  "product_version": "3.13.0",
  "users": [
    {
      "name": "heidi",
      "password_hash": "generated-from-env",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator",
      "limits": {}
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "heidi",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "heidi-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "ha-policy",
      "pattern": ".*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "exactly",
        "ha-params": 2,
        "ha-sync-mode": "automatic"
      },
      "priority": 0
    },
    {
      "vhost": "/",
      "name": "message-ttl",
      "pattern": "^notification\\.queue$",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 86400000
      },
      "priority": 1
    }
  ],
  "queues": [
    {
      "name": "user.created",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000
      }
    },
    {
      "name": "user.updated",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000
      }
    },
    {
      "name": "user.deleted",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000
      }
    },
    {
      "name": "notification.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000
      }
    },
    {
      "name": "scheduler.tasks",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "integration.webhooks",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "user.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "notification.events",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "scheduler.events",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "integration.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "user.events",
      "vhost": "/",
      "destination": "user.created",
      "destination_type": "queue",
      "routing_key": "user.created",
      "arguments": {}
    },
    {
      "source": "user.events",
      "vhost": "/",
      "destination": "user.updated",
      "destination_type": "queue",
      "routing_key": "user.updated",
      "arguments": {}
    },
    {
      "source": "user.events",
      "vhost": "/",
      "destination": "user.deleted",
      "destination_type": "queue",
      "routing_key": "user.deleted",
      "arguments": {}
    },
    {
      "source": "notification.events",
      "vhost": "/",
      "destination": "notification.queue",
      "destination_type": "queue",
      "routing_key": "notification.send",
      "arguments": {}
    },
    {
      "source": "scheduler.events",
      "vhost": "/",
      "destination": "scheduler.tasks",
      "destination_type": "queue",
      "routing_key": "task.execute",
      "arguments": {}
    },
    {
      "source": "integration.events",
      "vhost": "/",
      "destination": "integration.webhooks",
      "destination_type": "queue",
      "routing_key": "webhook.received",
      "arguments": {}
    }
  ]
}
